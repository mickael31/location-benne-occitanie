{
  "name": "location-benne-occitanie",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "desktop:dev": "concurrently -k -n VITE,ELECTRON \"npm:dev\" \"wait-on http://localhost:3000 && electron .\"",
    "desktop:pack": "npm run build && electron-packager . \"Location Benne Occitanie\" --platform=win32 --arch=x64 --out=release --overwrite --asar --prune=true"
  },
  "dependencies": {
    "react-router-dom": "^7.13.0",
    "react-helmet-async": "^2.0.5",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "electron": "^34.2.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.5"
  }
}
